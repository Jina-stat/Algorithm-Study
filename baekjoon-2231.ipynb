   "source": [
    "# 백준 2231\n",
    "\n",
    "n = int(input())\n",
    "answer = 0\n",
    "for i in range(n):\n",
    "    if sum(list(map(int, str(i)))) + i == n:\n",
    "        # 가장 작은 생성자를 찾아야함. \n",
    "        # 따라서 찾으면 break\n",
    "        answer = i\n",
    "        break\n",
    "\n",
    "# 생성자가 없으면 0 출력\n",
    "print(answer)\n",
    "\n",
    "\n",
    "\n"
   ]
